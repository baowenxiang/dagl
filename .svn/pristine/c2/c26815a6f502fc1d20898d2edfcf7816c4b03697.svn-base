<!DOCTYPE html>
<html>
<head>
	<!-- layer -->
	<link href="${rc.contextPath}/resources/plugins/layer/css/layui.css" rel="stylesheet">
	<link href="${rc.contextPath}/resources/css/web/common/exactquery.css" rel="stylesheet">
</head>
<body>
    <div class="site-text site-block">
		<form class="layui-form">
            <div class="layui-form-item yw">
                <label class="layui-form-label">题名</label>
                <div class="layui-input-block">
					<input type="text" name="tm" required  lay-verify="required" placeholder="请输入标题" autocomplete="off" class="layui-input">
                </div>
            </div>
			<div class="layui-form-item layui-form-text yw">
                <label class="layui-form-label">备注</label>
                <div class="layui-input-block">
                  <textarea name="bz" placeholder="请输入内容" class="layui-textarea"></textarea>
                </div>
            </div>
            <div class="layui-form-item yw">
                <div class="layui-input-block">
                    <button class="layui-btn" lay-submit lay-filter="confirm">确定</button>
                    <button type="reset" class="layui-btn layui-btn-primary">重置</button>
                </div>
            </div>
        </form>
    </div>
</body>
<script src="${rc.contextPath}/resources/plugins/layer/layui.js" type="text/javascript"></script>
