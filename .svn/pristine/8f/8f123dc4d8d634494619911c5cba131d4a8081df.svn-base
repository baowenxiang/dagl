<section class="content">
	<div class="box box-default">
		<div class="box-body">
      		##title区域
      		<ul class="layui-nav">
		  		<li class="layui-nav-item"><p>用户日志</p>
		  		</li>
			</ul>         	
            	
		    ##资料收集分页展示DTgrid
		    <div style="" id="dataCollectContext" >
		    	##查询条件
		    	<div id="conditionDataCollect">
		    	</div>
		    	##列表
		    	<div id="tableDataCollect" class="dt-grid-container"></div>
		    	##分页栏
		    	<div id="pagingDataCollect" class="dt-grid-toolbar-container"></div>
		    </div>
		</div>
	</div>
	##日志详情模态框
	<div class="modal" id="dataCollectUpdate" tabindex="-1" role="dialog" aria-labelledby="myModalLabel1" aria-hidden="true" data-show="true">
        <div class="modal-dialog">
            <div class="modal-content">
				##头部
    			<div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title" id="myModalLabel1">日志详情</h4>
                </div>
				##正文
				<div class="modal-body scroll" style="overflow: auto; min-height: 410px; max-height: 410px">
					<form class="form-horizontal" role="form">
    					<div class="form-group">
    						<label class="col-sm-3 control-label">时间</label>
    				    	<div class="col-sm-7">
    				        	<input name="time" class="form-control">
    				    	</div>
    				    </div>
    					<div class="form-group">
    						<label class="col-sm-3 control-label">日志类型</label>
    				    	<div class="col-sm-7">
    				    		<input class="form-control" name="type">
    				    	</div>
    				    </div>
    				    <div class="form-group">
    						<label class="col-sm-3 control-label">日志内容</label>
    				    	<div class="col-sm-7">
    						    <input class="form-control"  name="content">
    				    	</div>
    				    </div>
					   <div class="form-group">
    						<label class="col-sm-3 control-label">操作者</label>
    				    	<div class="col-sm-7">
    				    		<input class="form-control" name="userId">
    				    	</div>
    				    </div>
    				    #*<div class="form-group">
    						<label class="col-sm-3 control-label">操作者姓名</label>
    				    	<div class="col-sm-7">
    						    <input class="form-control"  name="userName">
    				    	</div>
    				    </div>*#
                     </form>
                </div>
				##尾部
				<div class="modal-footer">
                    <button type="button" class="btn btn-default modalClose" data-dismiss="modal">关闭</button>
                </div>
			</div>
        </div>
	</div>
</section>     
        
##CSS
<link href="${rc.contextPath}/resources/css/web/setting/userLog.css" rel="stylesheet" />
	
##Javascript
<script src="${rc.contextPath}/resources/js/web/setting/userLog.js"></script>
