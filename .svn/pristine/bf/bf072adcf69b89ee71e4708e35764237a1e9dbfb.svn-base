@CHARSET "UTF-8";
/*fixed page toolbar plugins style*/
#toolbar{
    width: 100%;
    height: 37px;
    line-height: 37px;
    border-bottom: 1px solid #747474;
    background-color: #F9F9F9;
    position: fixed;
    border-bottom: 1px solid #F3F3F3;
    padding: 0 6px;
    z-index: 1000;
}
#page_title{
	display: inline-block;
	font-size: 14px;
	font-weight: bold;
	padding-left: 10px;
}
#btnbar{
    text-align: right;
    width: 70%;
    float: right;
}
